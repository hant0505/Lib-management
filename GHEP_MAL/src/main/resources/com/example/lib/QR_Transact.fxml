<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/23" fx:controller="com.example.lib.PopUp_Controller">
    <VBox alignment="CENTER" spacing="10" AnchorPane.leftAnchor="10" AnchorPane.rightAnchor="10">
        <Label prefHeight="33.0" prefWidth="137.0" style="-fx-font-size: 16px;" text="Transaction Details" />
        <Label fx:id="transactionInfoLabel" style="-fx-font-size: 14px;" />
        <HBox alignment="CENTER" spacing="10">
            <Button fx:id="cancelButton" onAction="#onCancel" text="Cancel" />
            <Button fx:id="confirmButton" onAction="#onConfirm" text="Confirm" />
        </HBox>
    </VBox>
</AnchorPane>

